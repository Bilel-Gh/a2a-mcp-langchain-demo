{
  "name": "a2a-weather-translator-demo",
  "version": "1.0.0",
  "description": "DÃ©mo A2A avec Weather Agent + Translator Agent",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "weather": "node dist/agents/weather-agent/server.js",
    "translator": "node dist/agents/translator-agent/server.js",
    "web-search": "node dist/agents/web-search-agent/server.js",
    "calculator": "node dist/agents/calculator-agent/server.js",
    "travel-planner": "node dist/agents/travel-planner-agent/server.js",
    "demo": "node dist/client/travel-demo.js",
    "dev:weather": "tsx watch src/agents/weather-agent/server.ts",
    "dev:translator": "tsx watch src/agents/translator-agent/server.ts",
    "dev:web-search": "tsx watch src/agents/web-search-agent/server.ts",
    "dev:calculator": "tsx watch src/agents/calculator-agent/server.ts",
    "dev:travel-planner": "tsx watch src/agents/travel-planner-agent/server.ts",
    "dev:demo": "tsx src/client/travel-demo.ts",
    "dev": "concurrently -n \"weather,translator,web,calc,travel\" -c \"bgBlue,bgMagenta,bgGreen,bgYellow,bgCyan\" \"npm run dev:weather\" \"npm run dev:translator\" \"npm run dev:web-search\" \"npm run dev:calculator\" \"npm run dev:travel-planner\""
  },
  "dependencies": {
    "@a2a-js/sdk": "^0.3.4",
    "@google/generative-ai": "^0.21.0",
    "@langchain/core": "^0.3.78",
    "@langchain/google-genai": "^0.2.18",
    "@langchain/langgraph": "^0.4.9",
    "@langchain/mcp-adapters": "^0.6.0",
    "@modelcontextprotocol/sdk": "^1.20.0",
    "axios": "^1.7.2",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "uuid": "^10.0.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.0",
    "@types/uuid": "^10.0.0",
    "concurrently": "^8.2.2",
    "tsx": "^4.20.6",
    "typescript": "^5.3.3"
  }
}
